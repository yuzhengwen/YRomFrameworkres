<?xml version="1.0" encoding="utf-8"?>
<animated-selector android:constantSize="true" somc:extendedLookAndFeel="true"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:somc="http://schemas.android.com/apk/res/com.sonyericsson.uxp">
    <item android:state_enabled="false" android:drawable="@drawable/somc_seekbar_thumb_disabled_light" />
    <item android:state_pressed="true" android:id="@id/pressed" android:drawable="@drawable/somc_seekbar_thumb_to_pressed_5_light" />
    <item android:id="@id/not_pressed" android:drawable="@drawable/somc_seekbar_thumb_to_pressed_0_light" />
    <transition android:toId="@id/pressed" android:fromId="@id/not_pressed">
        <animation-list>
            <item android:duration="15" android:drawable="@drawable/somc_seekbar_thumb_to_pressed_0_light" />
            <item android:duration="15" android:drawable="@drawable/somc_seekbar_thumb_to_pressed_1_light" />
            <item android:duration="15" android:drawable="@drawable/somc_seekbar_thumb_to_pressed_2_light" />
            <item android:duration="15" android:drawable="@drawable/somc_seekbar_thumb_to_pressed_3_light" />
            <item android:duration="15" android:drawable="@drawable/somc_seekbar_thumb_to_pressed_4_light" />
            <item android:duration="15" android:drawable="@drawable/somc_seekbar_thumb_to_pressed_5_light" />
        </animation-list>
    </transition>
    <transition android:toId="@id/not_pressed" android:fromId="@id/pressed">
        <animation-list>
            <item android:duration="15" android:drawable="@drawable/somc_seekbar_thumb_from_pressed_0_light" />
            <item android:duration="15" android:drawable="@drawable/somc_seekbar_thumb_from_pressed_1_light" />
            <item android:duration="15" android:drawable="@drawable/somc_seekbar_thumb_from_pressed_2_light" />
            <item android:duration="15" android:drawable="@drawable/somc_seekbar_thumb_from_pressed_3_light" />
            <item android:duration="15" android:drawable="@drawable/somc_seekbar_thumb_from_pressed_4_light" />
            <item android:duration="15" android:drawable="@drawable/somc_seekbar_thumb_from_pressed_5_light" />
        </animation-list>
    </transition>
</animated-selector>